{
	"name": "Mini version of Colorpeek",
	"description": "Get every background color from the current tab",
	"version": "1.0",

	"manifest_version": 2,

	"icons": {
		"16": "icon16.png",
		"48": "icon48.png",
		"128": "icon128.png"
	},

	"background": {
		"scripts": ["event.js", "inject.js"],
		"persistent": false
	},

	"permissions": ["<all_urls>"]
}