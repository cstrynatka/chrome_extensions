{
	"manifest_version": 2,

	"name": "Jaws music for Donald Trump",
	"description": "The theme music for Jaws will start to play every time someone visits a page that has to
	do with Donald Trump",
	"version": "1.0",

	"background": {
		"scripts": ["background.js", "content.html", "content.js"]
	},

	"icons": {
		"16": "icon16.png",
		"48": "icon48.png",
		"128": "icon128.png"
	},

	"content_scripts": [
	{
		"matches": ["<all_urls>"],
		"js": ["content.js"]
		}
	],

	"browser_action": {
		// "default_icon": "icon.png"
		// "default_popup": "popup.html"
	},

	"permissions": [
		"activeTab",
		"https://ajax.googleapis.com/"
		]
	}